{
	"name": "imagemin-cli",
	"version": "7.0.0",
	"description": "Minify images seamlessly",
	"license": "MIT",
	"repository": "imagemin/imagemin-cli",
	"funding": "https://github.com/imagemin/imagemin-cli?sponsor=1",
	"type": "module",
	"bin": {
		"imagemin": "cli.js"
	},
	"engines": {
		"node": ">=12.20"
	},
	"scripts": {
		"test": "xo && ava"
	},
	"files": [
		"cli.js"
	],
	"keywords": [
		"cli",
		"cli-app",
		"compress",
		"imagemin",
		"gif",
		"image",
		"jpeg",
		"jpg",
		"minify",
		"png",
		"svg"
	],
	"dependencies": {
		"arrify": "^3.0.0",
		"get-stdin": "^9.0.0",
		"imagemin": "^8.0.0",
		"lodash.pairs": "^3.0.1",
		"meow": "^10.1.1",
		"ora": "^5.4.1",
		"plur": "^4.0.0",
		"strip-indent": "^4.0.0"
	},
	"devDependencies": {
		"ava": "^3.15.0",
		"execa": "^5.1.1",
		"imagemin-pngquant": "^9.0.2",
		"typescript": "^4.3.5",
		"xo": "^0.43.0"
	},
	"optionalDependencies": {
		"imagemin-gifsicle": "^7.0.0",
		"imagemin-jpegtran": "^7.0.0",
		"imagemin-optipng": "^8.0.0",
		"imagemin-svgo": "^9.0.0"
	},
	"xo": {
		"rules": {
			"node/process-exit-as-throw": 2,
			"node/no-unsupported-features": "off"
		}
	}
